{
  "name": "@blend-metrics/icons",
  "version": "0.1.0",
  "description": "Icons for React",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "icons/**/*",
    "brands/**/*",
    "social-solid/**/*",
    "social/**/*",
    "payment-methods/**/*",
    "flags/**/*",
    "README.md"
  ],
  "scripts": {
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "prepare": "husky install",
    "build:icons": "svgr --config-file ./configs/icons.config.js assets/icons",
    "build:social": "svgr --config-file ./configs/social.config.js assets/social",
    "build:social-solid": "svgr --config-file ./configs/social-solid.config.js assets/social-solid",
    "build:payment-methods": "svgr --config-file ./configs/payment-methods.config.js assets/payment-methods",
    "build:flags": "svgr --config-file ./configs/flags.config.js assets/flags",
    "build:brands": "svgr --config-file ./configs/brands.config.js assets/brands",
    "build:all-icons": "rimraf ./build && concurrently \"npm:build:icons\" \"npm:build:social\" \"npm:build:social-solid\" \"npm:build:payment-methods\" \"npm:build:flags\" \"npm:build:brands\"",
    "build:build": "rimraf ./icons ./brands ./social-solid ./social ./payment-methods ./flags && concurrently \"tsup build/icons/index.ts --format cjs,esm --dts --out-dir icons\" \"tsup build/brands/index.ts --format cjs,esm --dts --out-dir brands\" \"tsup build/social-solid/index.ts --format cjs,esm --dts --out-dir social-solid\" \"tsup build/social/index.ts --format cjs,esm --dts --out-dir social \" \"tsup build/flags/index.ts --format cjs,esm --dts --out-dir flags\"  \"tsup build/payment-methods/index.ts --format cjs,esm --dts --out-dir payment-methods\"",
    "build": "npm run build:all-icons && npm run build:build",
    "types:check": "tsc",
    "tooling": "concurrently \"npm:format:check\" \"npm:lint:check\" \"npm:build\" && npm run types:check",
    "release": "npm run tooling && changeset publish",
    "version": "changeset version"
  },
  "keywords": [
    "icons",
    "react",
    "storybook"
  ],
  "author": "Dheeraj Singh Nagdali <dheerajsinghnagdali@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@svgr/cli": "^8.1.0",
    "@swc/core": "^1.3.95",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "react": "18.x",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": "18.x"
  }
}
